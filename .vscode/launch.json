  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch init script",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceRoot}",
      "args": ["--pattern", "DesktopDateRangePicker"],
      "program": "${workspaceFolder}/init/init.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
