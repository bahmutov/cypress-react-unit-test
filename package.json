{
  "name": "cypress-react-unit-test",
  "version": "0.0.0-development",
  "description": "Unit test React components using Cypress",
  "main": "dist",
  "no-module-for-now": "lib",
  "scripts": {
    "test": "cypress run",
    "percy:test": "percy exec -- cypress run",
    "build": "webpack -d && npm run transpile",
    "cy:open": "cypress open",
    "transpile": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run lint && npm run transpile",
    "lint": "standard --verbose --fix *.js src cypress/integration plugins",
    "semantic-release": "semantic-release"
  },
  "files": [
    "dist",
    "lib",
    "plugins",
    "support"
  ],
  "types": "lib",
  "keywords": [
    "react",
    "cypress",
    "cypress-io",
    "test",
    "testing"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@percy/cypress": "1.0.9",
    "@types/node": "9.6.55",
    "axios": "0.19.2",
    "babel-loader": "8.1.0",
    "css-loader": "2.1.1",
    "cypress": "4.3.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "semantic-release": "17.0.4",
    "standard": "14.3.3",
    "style-loader": "0.23.1",
    "typescript": "3.8.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  },
  "standard": {
    "globals": [
      "Cypress",
      "cy",
      "expect"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/cypress-react-unit-test.git"
  },
  "dependencies": {
    "@cypress/webpack-preprocessor": "4.1.3",
    "debug": "4.1.1",
    "find-yarn-workspace-root": "1.2.1"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "use-bundler-from-cra",
        "prerelease": "use-bundler-from-cra"
      }
    ]
  }
}
